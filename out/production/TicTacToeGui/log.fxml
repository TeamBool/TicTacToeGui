            <Font name="Arial" size="13.0" />
         </font></TextField>
      <Label fx:id="lblstatus" alignment="CENTER" layoutX="300.0" layoutY="240.0" prefHeight="37.0" prefWidth="238.0" textAlignment="RIGHT" textFill="#8d0404">
