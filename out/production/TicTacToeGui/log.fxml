            <Font size="20.0" />
         </font>
      </Label>
      <Button alignment="TOP_CENTER" cache="true" contentDisplay="BOTTOM" layoutX="488.0" layoutY="303.0" mnemonicParsing="false" onAction="#backbutton" prefHeight="29.0" prefWidth="101.0" stylesheets="@styles/button.css" text="back" textAlignment="CENTER" textOverrun="LEADING_WORD_ELLIPSIS" />
